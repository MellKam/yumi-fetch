{
  "name": "yumi-fetch",
  "version": "0.1.2-beta.0",
  "description": "üç≠ An extensible fetch wrapper for simplified and powerful HTTP requests using native web APIs",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/mod.d.ts",
  "type": "module",
  "files": [
    "dist/",
    "src/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "author": {
    "name": "Artem Melnyk",
    "url": "https://github.com/MellKam"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/mod.d.ts"
    },
    "./addons": {
      "default": "./dist/addons.js",
      "import": "./dist/addons.js",
      "require": "./dist/addons.cjs",
      "types": "./dist/addons/mod.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "index": ["./dist/mod.d.ts"],
      "addons": ["./dist/addons/mod.d.ts"]
    }
  },
  "packageManager": "pnpm@8.6.1",
  "license": "MIT",
  "repository": "github:MellKam/yumi-fetch",
  "readme": "https://github.com/MellKam/yumi-fetch#readme",
  "homepage": "https://github.com/MellKam/yumi-fetch#readme",
  "bugs": {
    "url": "https://github.com/MellKam/yumi-fetch/issues"
  },
  "keywords": [
    "fetch",
    "client",
    "request",
    "response",
    "yumi",
    "api",
    "rest",
    "http",
    "ajax",
    "network",
    "middleware",
    "extensible",
    "plugin"
  ]
}
